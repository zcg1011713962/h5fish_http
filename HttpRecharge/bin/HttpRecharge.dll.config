    </system.web>

  <appSettings>
    <add key="anysdk" value="anysdk_pay" />
    <add key="anysdk_key" value="FE9F7C348110B2614026EBF1E03D749C" />
    
    <add key="checkOrder" value="http://localhost:4656/?playerId={0}&amp;orderId={1}&amp;payType={2}" />
